{
  "compilerOptions": {
    "target": "esnext",
    "module": "NodeNext",
    "outDir": "./build",
    "rootDir": "./src",
    "strict": true,
    "baseUrl": "./src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node16"
  }
}